<?xml version="1.0"?>
<robot name="demobot">
    <link name="base_link">
        <visual>
            <geometry>
                <box size="0.2 0.2 0.005"/>
            </geometry>
        </visual>
    </link>

    <joint name="caster joint" type="fixed">
        <parent link="base_link"/>
        <child link="caster"/>
        <origin xyz="-0.19 0 -0.03"/>
    </joint>

    <link name="caster">
        <visual>
            <geometry>
                <sphere radius="0.01"/>
            </geometry>
        </visual>
    </link>

    <joint name="left_wheel_joint" type="continuous">
        <parent link="base_link"/>
        <child link="left_wheel"/>
        <origin xyz="0.05 0.115 -0.01" rpy="1.571 0 0"/>
        <axis xyz="0 0 1"/>
    </joint>

    <link name="left_wheel">
        <visual>
            <geometry>
                <cylinder radius="0.03" length="0.03"/>
            </geometry>
        </visual>
    </link>


</robot>